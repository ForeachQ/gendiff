{
    "name": "hexlet/code",
    "type": "project",
    "description": "difference project",
    "license": "proprietary",
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "psr-4": {
            "Hexlet\\Code\\": "src/"
        },
        "files": [
            "src/DiffGenerator/Differ.php",
            "src/DiffGenerator/FindDifferences.php",
            "src/Parsers/JsonParser.php",
            "src/Parsers/YamlParser.php",
            "src/Parsers/ParserFactory.php",
            "src/Formatters/FormatterFactory.php",
            "src/Formatters/StylishFormatter.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Differ\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ilya Golovin",
            "email": "foreachq@gmail.com"
        }
    ],
    "require-dev": {
        "docopt/docopt": "^1.0",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5"
    },
    "require": {
        "ext-json": "*",
        "symfony/yaml": "^5.4"
    }
}
